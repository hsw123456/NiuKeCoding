package com.niuke.test;

import java.util.ArrayList;

public class Main {
	public static void main(String[] args) {
		
		int[] arr={2,4,3,6,8,5,1};
		
		ArrayList<Integer> list = new ArrayList<Integer>();
		for(int i =0; i<arr.length;i++){
			list.add(arr[i]);
		}
		
		System.out.println(list);
		
	}

}
